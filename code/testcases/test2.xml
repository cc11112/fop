<?xml version="1.0" encoding="iso-8859-1"?>

<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format">
  <fo:layout-master-set>
    <fo:simple-page-master master-name="my-page">
      <fo:region-body margin="1in"/>
    </fo:simple-page-master>
  </fo:layout-master-set>

  <fo:page-sequence master-reference="my-page">
    <fo:flow flow-name="xsl-region-body">
      <fo:block font-family="Morningstar 1 Light">Morningstar 1 Light</fo:block>
      <fo:block font-family="Morningstar Light">Morningstar Light</fo:block>
      <fo:block font-family="UniversLTStd-LightCn">UniversLTStd-LightCn</fo:block>
      <fo:block font-family="UniversCond">UniversCond</fo:block>
    </fo:flow>
  </fo:page-sequence>
</fo:root>